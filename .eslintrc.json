{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": [
            "error",
            "windows"
        ],
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": false,
                "optionalDependencies": false,
                "peerDependencies": false,
                "import/no-extraneous-dependencies": [
                    "error",
                    {
                        "devDependencies": false,
                        "optionalDependencies": false,
                        "peerDependencies": false
                    }
                ]
            }
        ]
    }
}

